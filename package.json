{
  "name": "klestr_art_engine",
  "version": "1.0.0",
  "description": "Klestr Art Engine is a tool used to create multiple different instances of artworks based on provided layers. shout out hashlips",
  "main": "index.js",
  "bin": "index.js",
  "pkg": {
    "assets": [
      "layers/**/*",
      "node_modules/**/*",
      "src/**/*"
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "dev": "tsc",
    "legacy:build": "node utils/legacy/index.js",
    "legacy:generate": "node utils/legacy/index.js",
    "legacy:rarity": "node utils/legacy/utils/rarity.js",
    "legacy:preview": "node utils/legacy/utils/preview.js",
    "legacy:pixelate": "node utils/legacy/utils/pixelate.js",
    "legacy:update_info": "node utils/legacy/utils/update_info.js",
    "legacy:preview_gif": "node utils/legacy/utils/preview_gif.js",
    "legacy:generate_metadata": "node utils/legacy/utils/generate_metadata.js"
  },
  "author": "Tawanda Kanyangarara (Klish3)",
  "license": "MIT",
  "dependencies": {
    "canvas": "^2.8.0",
    "gif-encoder-2": "^1.0.5",
    "gulp": "^4.0.2",
    "import": "^0.0.6",
    "sha1": "^1.1.1"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/node": "^17.0.23",
    "eslint": "^8.12.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.7.0-dev.20220405"
  }
}
